function addTab(name) {
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var sheet = ss.getSheetByName(name);
  if(null == sheet) {
    sheet = ss.insertSheet();
    sheet.setName(name)
    sheet.addDeveloperMetadata("ID",name);
  }
  return sheet;
}

